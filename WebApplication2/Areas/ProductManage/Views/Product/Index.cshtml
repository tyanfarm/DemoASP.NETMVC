@using WebApplication2.Areas.ProductManage.Models
@model List<ProductModel>

<h1 class="pb-4">Danh sách các sản phẩm</h1>

<div class="list-group" style="margin-bottom:150px;">
    @foreach (var product in @Model)
    {
        // asp-action là @planet.Name để bên Controller ta sẽ đặt từng action là từng tên planets
        <div class="list-group-item list-group-item-action">
            @product.Name
        </div>
    }
</div>
